<div data-role="page">

  <div data-role="header" data-position="inline">
    <h1>New <%= @productividad.title %></h1>
    <a href="<%= url_for :action => :index %>" class="ui-btn-left" data-icon="back" data-direction="reverse">
      Cancel
    </a>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :create %>">
      <input type="hidden" name="id" value="<%= @productividad.object %>"/>

      
          <div data-role="fieldcontain">
            <label for="productividad[cuf_pagable_filtrado]" class="fieldLabel">Cuf pagable filtrado</label>
            <input type="text" id="productividad[cuf_pagable_filtrado]" name="productividad[cuf_pagable_filtrado]" <%= placeholder("Cuf pagable filtrado") %> />
          </div>
      
          <div data-role="fieldcontain">
            <label for="productividad[horas_hombre]" class="fieldLabel">Horas hombre</label>
            <input type="text" id="productividad[horas_hombre]" name="productividad[horas_hombre]" <%= placeholder("Horas hombre") %> />
          </div>
      

      <input type="submit" value="Create"/>
    </form>
  </div>

</div>
