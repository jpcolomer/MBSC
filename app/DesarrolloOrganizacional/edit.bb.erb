<div data-role="page">

  <div data-role="header" data-position="inline">
    <%= link_to "Back", :action => :show, :id => @desarrollo_organizacional.object %>
    <h1>Edit <%= @desarrollo_organizacional.cuadros_reemplazo%></h1>
    <%= link_to "Delete", :action => :delete, :id => @desarrollo_organizacional.object %>
  </div>

  <div data-role="content">
    <form method="POST" action="<%= url_for :action => :update %>">
      <input type="hidden" name="id" value="<%= @desarrollo_organizacional.object %>"/>
      <table>
          
          <tr>
              <td class="itemLabel">Cuadros reemplazo: </td>
              <td class="itemValue"><input type="text" name="desarrollo_organizacional[cuadros_reemplazo]" value="<%= @desarrollo_organizacional.cuadros_reemplazo %>"/></td>
          </tr>
          
      </table>
      <input type="submit" value="Update"/>
    </form>
  </div>

</div>
